<div id="wrapper">
  <%=render 'partner/header'%>
  <!-- End Navbar -->
  <!-- Home
    ================================================== -->
  <section>
    <div id="home-section">
      <div class="home-content">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="intro-title">
                <h2>Partner with us</h2>
                <h4>Business opportunity with no upfront investment</h4>
              </div>
              <!-- end intro-title -->
              <div class="call-to-action">
                <a class="btn btn-cta" href="<%=new_property_path%>" target="_blank">Become a partner</a>
              </div>
              <!-- end call-to-action -->
            </div>
            <!-- end col-md-12 -->
          </div>
          <!-- end row -->
        </div>
        <!-- end container -->
      </div>
      <!-- end home-content -->
    </div>
    <!-- end home-section -->
  </section>
  <!-- End home section -->
  <!-- About 
    ================================================== -->
  <section>
    <div id="about-section">
      <div class="container">
        <div class="title-section animated out" data-animation="fadeInUp" data-delay="0">
          <h3>Convert your commercial property into a coworking hub </h3>
          <hr>
          <p>We help commercial property owners convert <br> their commercial office space into a coworking space</p>
        </div>
        <!-- end title-section -->
        <div class="about-us">
          <div class="row">
            <div class="col-md-7">
              <div class="about-img animated out" data-animation="fadeInLeft" data-delay="0">
                <img src="/assets/partner/temp/about-img.png" alt="">
              </div>
              <!-- end about-img -->
            </div>
            <!-- end col-md-7 -->
            <div class="col-md-5">
              <div class="about-text animated out" data-animation="fadeInRight" data-delay="0">
                <h1>No Investment required</h1>
                <p>If you have a vacant commercial office space and looking for a business opportunity then we have a perfect option for you.We would build your commercial space into a coworking hub and help you in managing it through our space management system. We would be bringing the clients, you just have to manage the space.</p>
                <div class="about-action-btns">
                  <a href="<%=new_property_path%>" class="btn btn-clean">Become a partner</a>
                  <!-- <a href="#" class="btn btn-clean">What is here?</a> -->
                </div>
                <!-- end about-action-btns -->
              </div>
              <!-- end about-text -->
            </div>
            <!-- end col-md-5 -->
          </div>
          <!-- end row -->
        </div>
        <!-- end about-us -->
      </div>
      <!-- end container -->
    </div>
  </section>
  <!-- end about section -->
