<div id="wrapper">
  <%=render 'partner/header'%>
  <!-- End Navbar -->
  <!-- Home
    ================================================== -->
  <section>
    <div id="home-section">
      <div class="home-content">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="intro-title">
                <h2 style="font-size: 42px;">Partner with us</h2>
                <h4>Let Aboard Offices work for your Space</h4>
              </div>
              <!-- end intro-title -->
              <div class="call-to-action">
                <a class="btn btn-cta" href="<%=new_property_path%>" target="_blank">Become a partner</a>
              </div>
              <!-- end call-to-action -->
            </div>
            <!-- end col-md-12 -->
          </div>
          <!-- end row -->
        </div>
        <!-- end container -->
      </div>
      <!-- end home-content -->
    </div>
    <!-- end home-section -->
  </section>
  <!-- End home section -->
  <!-- About 
    ================================================== -->
  <section>
    <div id="about-section">
      <div class="container">
        <div class="title-section animated out" data-animation="fadeInUp" data-delay="0">
          <h3>List your coworking Space with Aboard Offices </h3>
          <hr>
          <p>Aboard offices is a one stop destination for anyone looking for flexible workspace </p>
        </div>
        <!-- end title-section -->
        <div class="about-us">
          <div class="row">
            <div class="col-md-7">
              <div class="about-img animated out" data-animation="fadeInLeft" data-delay="0">
                <img src="/assets/partner/temp/about-img.png" alt="">
              </div>
              <!-- end about-img -->
            </div>
            <!-- end col-md-7 -->
            <div class="col-md-5">
              <div class="about-text animated out" data-animation="fadeInRight" data-delay="0">
                <h1>Join the Network </h1>
                <p>Expand your reach to workforce across different locations and get the visibility you are looking for through our platform. We make your space available to thousands of members who are searching for workplace, meeting rooms, training rooms. </p>
                <div class="about-action-btns">
                  <a href="<%=new_property_path%>" class="btn btn-clean">Become a partner</a>
                  <!-- <a href="#" class="btn btn-clean">What is here?</a> -->
                </div>
                <!-- end about-action-btns -->
              </div>
              <!-- end about-text -->
            </div>
            <!-- end col-md-5 -->
          </div>
          <!-- end row -->
        </div>
        <!-- end about-us -->
      </div>
      <!-- end container -->
    </div>
  </section>
  <!-- end about section -->
