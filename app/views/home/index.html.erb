<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active"> <img src="/assets/new_homepage/slider1.png" style="width:100%" alt="First slide">
      <div class="container">
        <div class="carousel-caption">
          <h1>Offices With Culture</h1>
          <p>Coworking, Community, Sharing!!! Come And Work At A Happy Place.</p>
        </div>
      </div>
    </div>
    <div class="item"> <img src="/assets/new_homepage/slider1.png" style="width:100%" data-src="" alt="Second    slide">
      <div class="container">
        <div class="carousel-caption">
          <h1>Offices With Culture</h1>
          <p>Coworking, Community, Sharing!!! Come And Work At A Happy Place. </p>
        </div>
      </div>
    </div>
    <div class="item"> <img src="/assets/new_homepage/slider1.png" style="width:100%" data-src="" alt="Third slide">
      <div class="container">
        <div class="carousel-caption">
          <h1>Offices With Culture</b></h1>
          <p>Coworking, Community, Sharing!!! Come And Work At A Happy Place.</p>
        </div>
      </div>
    </div>
  </div>
  <div class="search-list-box">
    <div class="col-md-10 col-md-offset-1 col-sm-12">
      <div  class="inner-bg">
        <form method="get" action="<%=properties_path%>">
          <div class="col-md-3 col-sm-3 padd-right">
            <label>Location</label>
              <input type="text" placeholder="Enter your desire locality" id="search_desire" name="desire_location">
            </input>
          </div>
          <div class="col-md-3  col-sm-3 padd-right">
            <label>City</label>
            <div class="styled-select blue semi-square home_index_slider">
              <select name="search_city" id="search_city">
                <option class="opt-default" selected="selected" disabled="disabled">Select City</option>
                <option label="Delhi" value="Delhi">Delhi</option>
                <option label="Indore" value="Indore">Indore</option>
                <option label="Bhopal" value="Bhopal">Bhopal</option>
                <option label="Chennai" value="Chennai">Chennai</option>
                <option label="Bangalore" value="Bangalore">Bangalore</option>
                <option label="Mumbai" value="Mumbai">Mumbai</option>
                <option label="Pune" value="Pune">Pune</option>
              </select>
            </div>
          </div>
          <div class="col-md-3  col-sm-3 padd-right">
            <label>Office Type</label>
            <div >
              <div >
                <select class="multiselect-ui form-control" name="search_office_type[]" id="dates-field2" multiple="" required="required">
                  <% PropertyType.all.each do |type| %>
                    <option value="<%=type.name%>"><%=type.name%></option>
                  <%end%>
                </select>
              </div>
            </div>
          </div>
          <div class="col-md-3  col-sm-3 padd-right">
            <label class="display">&nbsp;</label>
            <div class="col-md-6 home_index_search_left"><button type="submit" class="search-btn">Search</button></div>
            <input type="hidden" id="near_me" name="near_me" value="" />
            <div class="col-md-6 home_index_search_right"><button type="submit" class="search-btn" id="search_near_me">Near me</button></div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

