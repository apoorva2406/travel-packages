  <!--homegrids start here-->
  <% if @properites.present? && @properites.size > 2 %>
    <div class="home-block">
      <div class="container">
        <div class="home-main">
          <div class="home-top">
            <h2>THE BEST <span>LOCATIONS</span></h2>
            <hr>
          </div>
          <div class="demo">
            <div >
              <div class="row">
                <div class="col-md-12">
                  <%if @properites.present? %>
                    <div id="news-slider" class="owl-carousel">
                      <% @properites.take(6).each do |property|%>
                        <div class="post-slide">
                          <div class="post-img"> 
                            <a href="<%=property_path(property)%>"><img src="<%=property.try(:photos).first.try(:image).url%>" alt=""/></a>
                            <div class="city"><%=property.try(:address)%></div>
                          </div>
                          <div class="post-content">
                            <h3 class="post-title"><a href="<%=property_path(property)%>"><%=truncate(property.try(:name),length: 25)%></a></h3>
                            <p class="post-description"> <%=property.try(:description)%> </p>
                            <h4><span>From</span>RS <%=property.try(:property_prices).first.price%></h4>
                            <ul class="post-bar">
                              <%facilities = Facility.where(id: JSON(property.facilities))%>
                              <%facilities.each do |facility|%> 
                                <li><i class="fa fa-check-circle" aria-hidden="true"></i> <%=facility.try(:name)%></li>
                              <%end%>
                            </ul>
                            <div class="clearfix"></div>
                            <center>
                              <a href="<%=property_path(property)%>" class="read-more">read more</a>
                            </center>
                          </div>
                        </div>
                      <%end%>
                    </div>
                  <%end%>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  <%end%>
