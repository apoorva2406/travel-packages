<section class="top-content">
  <div class="container-slider removeslide">
    <div class="container-reservation inside-slider">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <!-- Reservation form -->
            <section id="reservation-form" class="reservation-color-form pos-inside-slide">
              <div class="container-form-chose">
                <div class="col-md-12">
                  <!--********************* Hotel reservation ********************-->
                  <form id="hotels-tab" class="tab-pane form-inline reservation-hotel active" method="get" name="reservationform" action="<%=properties_path%>">
                    
                    <div class="col-md-2">
                      <h3>City</h3>
                      <div class="form-group">
                        <select class="form-control" name="search_city" id="search_city">
                          <option class="opt-default" selected="selected" disabled="disabled">Select City</option>
                          <option label="Delhi" value="Delhi">Delhi</option>
                          <option label="Indore" value="Indore">Indore</option>
                          <option label="Bhopal" value="Bhopal">Bhopal</option>
                          <option label="Chennai" value="Chennai">Chennai</option>
                          <option label="Bangalore" value="Bangalore">Bangalore</option>
                          <option label="Mumbai" value="Mumbai">Mumbai</option>
                          <option label="Pune" value="Pune">Pune</option>
                          <option label="Bhind" value="Bhind">Bhind</option>
                        </select>
                      </div>
                    </div>

                    <div class="col-md-3">
                      <div class="form-group">
                        <h3>Desire locality</h3>
                        <input type="text" class="form-control" placeholder="Enter your desire locality" id="search_desire" name="desire_location">
                      </div>
                    </div>


                    <div class="col-md-3">
                      <h3>Office Type</h3>
                      <div class="form-group">
                        <select class="form-control" name="search_office_type[]" id="search_office_type" multiple="">
                          <% PropertyType.all.each do |type| %>
                            <option value="<%=type.name%>"><%=type.name%></option>
                          <%end%>
                        </select>
                      </div>
                    </div>

                    <div class="col-md-2 colbtn">
                      <h3>&nbsp;</h3>
                      <button type="submit" class="btn btn-primary btn-block">Search</button>
                    </div>

                    <div class="col-md-2 colbtn">
                      <h3>&nbsp;</h3>
                      <input type="hidden" id="near_me" name="near_me" value="" />
                      <button type="submit" class="btn btn-primary btn-block" id="search_near_me">Near me</button>
                    </div>

                  </form>
                </div>
              </div>
            </section>
          </div>
        </div>
      </div>
    </div>

    <div class="home-page removeslide">
